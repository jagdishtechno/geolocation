
	<div class="userListsWrapper">
	<h2>Users :</h2>
	<table class="table">
		<tr>
			<th>Id</th>
			<th>Name</th>		
			<th>Action</th>
		</tr>
		<% @users.each do |user| %>
		<tr>
			<td><%=user.id %></td>
			<td><%=user.username %></td>
			<td><a href="/delete/<%=user.id %>" class="link" onclick="return confirm('Sure to remove?');">Delete</a></td>
		</tr>
		<% end %>
	</table>

	</div>
